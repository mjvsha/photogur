<h1> Add a photo to photogur</h1>
<%= link_to("Back to Pictures", '/pictures') %>
<%= form_for(@picture) do |f| %>
  <p>
    <%= f.label :artist %> <br>
    <%= f.text_field :artist %>
  </p>

  <p>
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :url %> <br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>

<%end %>

<% if @picture.errors.any? %>
  <div id = "error-explanation">
    <h2><%= pluralize(@picture.errors.count, "error") %> prohibited this article from being saved </h2>

    <ul>
      <% @picture.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
    <%end%>

